grenbo="\e[92;1m"
NC='\033[0m'
clear
echo -e "\033[1;93mâ”Œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”\033[0m"
echo -e "\033[1;93mâ”‚$NC            MENU MANAGER VME    $NC "
echo -e "\033[1;93mâ””â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”˜\033[0m"
echo -e "\033[1;93mâ”Œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”\033[0m"
echo -e "\033[1;93mâ”‚  ${grenbo}01.${NC} Create Account Vmess ${NC}" 
echo -e "\033[1;93mâ”‚  ${grenbo}02.${NC} Triall Account Vmess ${NC}" 
echo -e "\033[1;93mâ”‚  ${grenbo}03.${NC} Delete Account Vmess ${NC}"
echo -e "\033[1;93mâ”‚  ${grenbo}04.${NC} Renew Masa Aktif Vmess ${NC}" 
echo -e "\033[1;93mâ”‚  ${grenbo}05.${NC} Check Account Login Vmess ${NC}" 
echo -e "\033[1;93mâ”‚  ${grenbo}06.${NC} Show Account Member ${NC}" 
echo -e "\033[1;93mâ”‚  ${grenbo}07.${NC} List N Detail's Vmess${NC}" 
echo -e "\033[1;93mâ”‚  ${grenbo}08.${NC} Limit Speed${NC}" 
echo -e "\033[1;93mâ”‚  ${grenbo}09.${NC} Limit Ip${NC}"
echo -e "\033[1;93mâ”‚  ${grenbo}10.${NC} Delete User Expired${NC}"
echo -e "\033[1;93mâ”‚  ${grenbo}11.${NC} Lock Account Vmess ${NC}"
echo -e "\033[1;93mâ”‚  ${grenbo}12.${NC} Unlock Account Vmess ${NC}"
echo -e "\033[1;93mâ”‚  ${grenbo}13.${NC} Renew ip Vmess ${NC}"
echo -e "\033[1;93mâ”‚  ${grenbo}14.${NC} Renew Quota Vmess ${NC}"    
echo -e "\033[1;93mâ”‚  ${grenbo}00.${NC} ComeBack${NC}" 
echo -e "\033[1;93mâ””â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”˜\033[0m"
echo -e ""
read -p "Select options >>>   " menu
case $menu in
1) clear ;
    addws
    ;;
2) clear ;
    trialws
    ;;
3) clear ;
    delws1
    ;;
4) clear ;
    renewws
    ;;
5) clear ;
    cekws
    ;;
