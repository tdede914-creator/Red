    xp.sh
    ;;
11) clear ;
   lock-vm
   ;;
12) clear ;
   unlock-vm
    ;;
13) clear ;
   ganti-ip-vmess
    ;;
14) clear ;
   change-vmess-qouta
    ;;
0) clear ;
   menu
    ;;
esac
